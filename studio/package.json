{
  "name": "biominer-indexd-studio",
  "version": "0.1.0",
  "private": true,
  "description": "BioMiner Indexd is a hash-based data indexing and tracking service providing globally unique identifiers.",
  "scripts": {
    "publish-code": "cross-env UMI_APP_API_PREFIX=https://api.3steps.cn/biominer-indexd max build && ossutilmac64 cp -r -u ./dist/ oss://indexd-org/ -c ~/.ossutilconfig-tgmc -e oss-ap-southeast-1.aliyuncs.com",
    "build:embed": "cross-env UMI_ENV=embed max build",
    "start:remote-dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev UMI_APP_API_PREFIX=https://api.3steps.cn/biominer-indexd max dev",
    "start:local-dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev UMI_APP_API_PREFIX=http://localhost:3001 max dev",
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "max build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "jest": "jest",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "openapi": "max openapi",
    "prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "preview": "npm run build && max preview --port 8000",
    "record": "cross-env NODE_ENV=development REACT_APP_ENV=test max record --scene=login",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev max dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev",
    "test": "jest",
    "test:coverage": "npm run jest -- --coverage",
    "test:update": "npm run jest -- -u",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/plots": "^2.4.0",
    "@ant-design/pro-card": "^2.9.9",
    "@ant-design/pro-descriptions": "^2.6.9",
    "@ant-design/pro-form": "^2.31.9",
    "@ant-design/pro-layout": "^7.22.6",
    "@ant-design/pro-table": "^3.20.1",
    "@ant-design/x": "^1.4.0",
    "@antv/l7": "^2.22.5",
    "@antv/l7-maps": "^2.22.5",
    "@glideapps/glide-data-grid": "^6.0.3",
    "@umijs/route-utils": "^2.0.0",
    "antd": "5.26.2",
    "classnames": "^2.3.0",
    "lodash": "^4.17.0",
    "markdown-it": "^14.1.0",
    "moment": "^2.29.0",
    "muuri": "^0.9.5",
    "omit.js": "^2.0.2",
    "papaparse": "^5.5.3",
    "plotly.js": "^3.0.1",
    "rc-menu": "^9.1.0",
    "rc-resize-observer": "^1.4.3",
    "rc-util": "^5.16.0",
    "re-resizable": "^6.11.2",
    "react": "^18.0.0",
    "react-dev-inspector": "^1.7.0",
    "react-dom": "^18.0.0",
    "react-grid-layout": "^1.5.1",
    "react-helmet-async": "^1.2.0",
    "react-laag": "^2.0.5",
    "react-markdown": "^8.0.3",
    "react-plotly.js": "^2.6.0",
    "react-responsive-carousel": "^3.2.23",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "semver": "^7.7.2"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@testing-library/react": "^13.4.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.21",
    "@types/history": "^4.7.11",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/lodash.debounce": "^4.0.9",
    "@types/markdown-it": "^14.1.2",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-helmet": "^6.1.11",
    "@types/react-window": "^1.8.8",
    "@umijs/fabric": "^2.14.1",
    "@umijs/lint": "^4.1.0",
    "@umijs/max": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "express": "^4.18.2",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mockjs": "^1.1.0",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.8",
    "prettier-plugin-two-style-order": "^1.0.1",
    "react-dev-inspector": "^1.9.0",
    "swagger-ui-dist": "^4.19.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "umi-presets-pro": "^2.0.3"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
